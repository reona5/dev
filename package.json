    "@fontsource/inter": "^4.5.15",
    "@tailwindcss/typography": "^0.5.9",
    "@vercel/analytics": "^1.0.0",
    "astro": "^2.3.2",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@markuplint/astro-parser": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.1",
